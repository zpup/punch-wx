@import "colorui/main.wxss";
@import "colorui/icon.wxss";



.modal-box {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0px;
    background: rgba(0, 0, 0, 0.6);
    overflow: hidden;
    z-index: 999999;
}

.modal-body {
    position: relative;
}


.modal-content {
    /* width: 60%; */
    margin: 70% 10% 0 20%;
    overflow: hidden;
    border-radius: 10rpx;
}

.modal-content {
    width: 60%;
    height: 150px;
}

.modal-content .fenxiang {
    width: 100%;
    height: 40%;
}